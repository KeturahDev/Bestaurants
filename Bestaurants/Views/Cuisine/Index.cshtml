@{Layout = "_Layout";}

<h1>Welcome to Best Restaurant</h1>
<hr>

@using Bestaurant.Models;
@model List<Bestaurant.Models.Cuisine>;

<h2>Possible Cuisines</h2>

@if(@Model.Count == 0)
{
  <p>Theres currently no entered cuisines.."</p>
}
@foreach(Cuisine cuisine in Model)
{
  <li>@cuisine.Name</li> //use html helper method for action link to details
}